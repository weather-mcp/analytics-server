{
  "dashboard": {
    "title": "Worker & Queue - Analytics Server",
    "tags": ["analytics", "worker", "queue"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "queue_depth",
            "legendFormat": "Queue Depth",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Events in Queue"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [1000], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "type": "query"
            }
          ]
        }
      },
      {
        "id": 2,
        "title": "Events Processed (events/min)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(events_processed_total{status=\"success\"}[5m]) * 60",
            "legendFormat": "Successful",
            "refId": "A"
          },
          {
            "expr": "rate(events_processed_total{status=\"error\"}[5m]) * 60",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Events/min"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Worker Batch Size Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(worker_batch_size_bucket[5m]))",
            "legendFormat": "p50 Batch Size",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(worker_batch_size_bucket[5m]))",
            "legendFormat": "p95 Batch Size",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Batch Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Worker Processing Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(events_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(events_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(events_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Processing Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Worker Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(worker_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Queue Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(queue_operations_total[5m])",
            "legendFormat": "{{operation}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Current Queue Depth",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "queue_depth",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Total Events Processed (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "sum(increase(events_processed_total{status=\"success\"}[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 9,
        "title": "Worker Error Rate (%)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "100 * (sum(rate(events_processed_total{status=\"error\"}[5m])) / sum(rate(events_processed_total[5m])))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Average Batch Size",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "avg(worker_batch_size)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
  }
}
